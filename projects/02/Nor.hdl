 /**
 * Nor gate:
 * out = 1 if (a == 0 or b == 0)
 *       0 otherwise
 */

CHIP Nor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=a, b=b, out=outor);
    Not (in=outor,out=out); 
}
